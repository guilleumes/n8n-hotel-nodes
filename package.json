{
  "name": "@guilleumes/n8n-hotel-nodes",
  "version": "1.0.0",
  "description": "Professional n8n custom nodes for hotel management: Beds24, Cuentica, TtLock, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package",
    "beds24",
    "cuentica",
    "hotel",
    "pms",
    "hospitality",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/guilleumes/n8n-hotel-nodes#readme",
  "author": {
    "name": "Guilleumes Team",
    "email": "dev@guilleumes.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guilleumes/n8n-hotel-nodes.git"
  },
  "bugs": {
    "url": "https://github.com/guilleumes/n8n-hotel-nodes/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "pnpm run clean && tsc && pnpm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "watch": "tsc --watch",
    "dev": "tsc --watch",
    "rebuild": "pnpm run clean && pnpm run build",
    "clean": "rimraf dist",
    "format": "prettier --write \"nodes/**/*.ts\" \"credentials/**/*.ts\" \"src/**/*.ts\"",
    "format:check": "prettier --check \"nodes/**/*.ts\" \"credentials/**/*.ts\" \"src/**/*.ts\"",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "pnpm run build",
    "test": "echo \"No tests yet\""
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Beds24Api.credentials.js",
      "dist/credentials/CuenticaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Beds24/Beds24.node.js",
      "dist/nodes/Beds24/Beds24Paginated.node.js",
      "dist/nodes/Beds24/Beds24Trigger.node.js",
      "dist/nodes/Cuentica/Cuentica.node.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "n8n-core": "^1.68.0",
    "n8n-workflow": "^1.68.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}
