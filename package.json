{
  "name": "@guilleumes/n8n-hotel-nodes",
  "version": "1.0.0",
  "description": "Professional n8n custom nodes for hotel management: Beds24, Cuentica, TtLock, and more",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package",
    "beds24",
    "cuentica",
    "hotel",
    "pms",
    "hospitality",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/guilleumes/n8n-hotel-nodes#readme",
  "author": {
    "name": "Guilleumes Team",
    "email": "dev@guilleumes.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guilleumes/n8n-hotel-nodes.git"
  },
  "bugs": {
    "url": "https://github.com/guilleumes/n8n-hotel-nodes/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "watch": "tsc --watch",
    "rebuild": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Beds24Api.credentials.js",
      "dist/credentials/CuenticaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Beds24/Beds24.node.js",
      "dist/nodes/Beds24/Beds24Paginated.node.js",
      "dist/nodes/Beds24/Beds24Trigger.node.js",
      "dist/nodes/Cuentica/Cuentica.node.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "1.82.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "typescript": "~4.8.4"
  }
}
